{
  "name": "ringcentral.ts",
  "version": "0.1.0",
  "scripts": {
    "build": "./scripts/build.sh",
    "watch": "npm run build -- watch",
    "test": "npm run build && npm run mocha",
    "mocha": "./node_modules/.bin/mocha ./build/tests/specs.js",
    "istanbul": "./node_modules/.bin/istanbul cover -x specs.js --dir=./build/coverage --report=lcov --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --reporter spec ./build/test/specs.js",
    "generate": "mkdir -p src/generated/url-builders; node codegen/generate.js  --out src/generated codegen/swagger.json",
    "docs": "typedoc --mode file --out ./build/api-docs/ src/ --module es6 -t es6 --exclude **/src/test/**/*.ts --includeDeclarations",
    "lint": "tslint -c tslint.json codegen/ts-sample/*.ts src/**/*.ts"
  },
  "dependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "@types/node": "^7.0.5",
    "form-data": "^2.1.2",
    "fs-promise": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "known-fetch-body": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "istanbul": "^0.4.0",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "tslint": "^4.4.2",
    "typedoc": "^0.5.6",
    "typescript": "^2.0.0",
    "webpack": "^2.2.1"
  },
  "preferGlobal": false,
  "private": false,
  "main": "./build/src/Client.js",
  "typings": "./src/Client.ts",
  "license": "MIT"
}
